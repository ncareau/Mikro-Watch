{
    "name": "ncareau/mikro-watch",
    "description": "Get and push data from Mikrotik Accounting to influxdb.",
    "homepage": "https://github.com/ncareau/mikro-watch",
    "license": "MIT",
    "authors": [
        {
            "name": "NMC",
            "email": "n@nmc.dev"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "symfony/console": "^3.4",
        "symfony/debug": "^3.4",
        "symfony/var-dumper": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "guzzlehttp/guzzle": "^6.3",
        "s1lentium/iptools": "^1.1",
        "influxdb/influxdb-php": "^1.15",
        "wrep/daemonizable-command": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit tests --coverage-clover=coverage.xml"
    }
